<%= form_for @user do |f| %>

  <ol>
      <% @user.errors.full_messages.each do |msg| %>
        <strong><%= msg %></strong><br>
      <% end %>
    </ol>

  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name%><br>
  <%= f.label :street_name, "Address"%>
  <%= f.text_field :street_name %><br>
  <%= f.label :city_name, "City" %>
  <%= f.text_field :city_name%><br>
  <%= f.label :state, "State" %>
  <%= f.text_field :state%><br>
  <%= f.label :zipcode, "ZIP Code" %>
  <%= f.text_field :zipcode%><br>
  <%= f.label :phone_num, "Phone" %>
  <%= f.telephone_field :phone_num %><br>
  <%= f.label :credit_card_num, "Credit Card Number" %>
  <%= f.text_field :credit_card_num %><br>

<br></br>
  <%= f.submit %>
  <% end %>
